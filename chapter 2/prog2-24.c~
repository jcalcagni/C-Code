#include <stdio.h>

main()
{
	int num1, num2;
	
	printf ("Enter an integer: \n");
	scanf ("%d", &num1);

	printf ("Enter another integer: \n");
	scanf ("%d", &num2);
	
	if (num1 % num2 == 0)
	   printf("Integer 1 is a multiple of Integer 2.\n");
	
/*	if (num1 % 2 != 0)
	   printf("Integer is odd.\n");
*/
	
}

