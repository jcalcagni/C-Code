#include <stdio.h>

main()
{
	printf ("   JJ\n");
	printf (" J   \n");
	printf ("J    \n");
	printf (" J   \n");
	printf ("  JJJJJJJ\n");
	printf ("   \n");
	printf ("MMMMMMMMM\n");
	printf ("       M \n");
	printf ("     M  \n");	
	printf ("       M\n");
	printf ("MMMMMMMMM\n");
	printf ("   \n");
	printf ("  CCCCCCCC\n");
	printf (" C        C \n");
	printf ("C          C \n");
	printf (" C        C \n");
	printf ("   \n");

}

